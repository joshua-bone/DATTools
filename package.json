{
  "name": "dattools",
  "version": "1.0.0",
  "description": "Toolkit for Chip's Challenge DAT levelsets (multiple 32x32 levels per file).",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p web/tsconfig.json --noEmit",
    "test:watch": "vitest",
    "dev:web": "vite",
    "build:web": "vite build",
    "preview:web": "vite preview",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "fmt:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshua-bone/DATTools.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/joshua-bone/DATTools/issues"
  },
  "homepage": "https://github.com/joshua-bone/DATTools#readme",
  "dependencies": {
    "commander": "^14.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "bin": {
    "dattools": "dist/cli.js"
  }
}
